def py_tree(source_tree, reverse=False, default=None):
    if not reverse:
        result = {}
        for i in range(len(source_tree.Branches)):
            d = source_tree.Branches[i]
            p = GH_Path(source_tree.Paths[i])
            
            result[p] = list(d)
        if len(result.keys())==0:
