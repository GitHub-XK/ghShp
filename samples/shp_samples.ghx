enc = py_tree(enc, default="utf-8")

def shift_path(d):
    new_dict = {}
    for k,v in d.items():
        _k = k.CullElement()
        if _k not in new_dict.keys():
            new_dict[_k] = []
        new_dict[_k].append(v)
    return new_dict


